@top Program { expression* }

@skip { space | Comment }

expression {
  Identifier |
  String |
  Boolean |
  Application { "(" expression* ")" }
}

@tokens {
  Identifier { $[a-zA-Z_\-0-9]+ }

  String { '"' (!["\\] | "\\" _)* '"' }

  Boolean { "#t" | "#f" }

  Comment { "(" ![)]* ")" }

  space { $[ \t\n\r]+ }

  "(" ")"
}

@detectDelim
